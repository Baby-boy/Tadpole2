<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!--数据库驱动 -->
	<classPathEntry
		location="F:\Work\mysql-connector-java-5.1.38-bin.jar" />
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
			<property name="defaultModelType" value="hierarchical" />
		</commentGenerator>
		<!--数据库链接地址账号密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.111.143:3306/gcj" userId="root" password="root">
		</jdbcConnection>
		<!--<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
			connectionURL="jdbc:oracle:thin:@127.0.0.1:1521:ORCL" userId="YCTTEST" password="YAMATOYCT">
		</jdbcConnection>-->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 生成Model类存放位置 -->
		<javaModelGenerator targetPackage="com.yd.gcj.entity" targetProject="src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 生成映射文件存放位置 -->
		<sqlMapGenerator targetPackage="mybatis" targetProject="src">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 生成Dao类存放位置 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.yd.gcj.mapper" targetProject="src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="yd_tpmsg" domainObjectName="YdMangerTPMsg" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		
		<table tableName="T_RECRUIT_PLAN" domainObjectName="RecruitPlan" schema=""></table>

	</context>
</generatorConfiguration>